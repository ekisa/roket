        <addForeignKeyConstraint baseColumnNames="mahalleler_id"
                                 baseTableName="merkez"
                                 constraintName="fk_merkez_mahalleler_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mahalle"/>

    </changeSet>
</databaseChangeLog>
