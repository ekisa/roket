        <addForeignKeyConstraint baseColumnNames="gps_lokasyon_id"
                                 baseTableName="kurye"
                                 constraintName="fk_kurye_gps_lokasyon_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gps_lokasyon"/>

    </changeSet>
</databaseChangeLog>
